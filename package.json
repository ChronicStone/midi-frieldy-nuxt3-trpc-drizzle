    "@trpc/client": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@unocss/reset": "^0.55.0",
    "@vuelidate/validators": "^2.0.3",
    "@vueuse/nuxt": "^10.3.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "consola": "^3.2.3",
    "dayjs": "^1.11.9",
